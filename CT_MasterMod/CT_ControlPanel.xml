		<Frames>
			<Slider name="$parentSlider" inherits="OptionsSliderTemplate">
				<Size>
					<AbsDimension x="150" y="17"/>
				</Size>
				<Anchors>
					<Anchor point="CENTER"/>
				</Anchors>
				<Scripts>
					<OnLoad>
						this:SetFrameLevel(4);
					</OnLoad>
					<OnValueChanged>
						if ( this.onChangeFunc ) then
							this.onChangeFunc(this.id, this:GetValue());
						end
						if ( this.showValueInTitle ) then
							getglobal(this:GetName() .. "Text"):SetText(this.title .. " - " .. format(this.showValueInTitle, floor(this:GetValue()*100)/100));
						else
							getglobal(this:GetName() .. "Text"):SetText(this.title);
						end
					</OnValueChanged>
				</Scripts>
			</Slider>
		</Frames>
